{
  "name": "@rn-ai-screenshot/client",
  "version": "0.1.0",
  "description": "React Native client SDK for AI screenshot functionality",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "clean": "rimraf dist"
  },
  "keywords": [
    "react-native",
    "ai",
    "screenshot",
    "sdk",
    "client"
  ],
  "author": "React Native AI Screenshot SDK Team (Generated by Kiro AI)",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-native": ">=0.60.0"
  },
  "dependencies": {
    "react-native-view-shot": "^3.8.0",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-native": "^0.72.0",
    "@types/ws": "^8.5.0",
    "@testing-library/react-native": "^12.1.0",
    "jest": "^29.5.0",
    "react": "^18.2.0",
    "react-native": "^0.72.0",
    "react-test-renderer": "^18.2.0",
    "rimraf": "^5.0.1",
    "typescript": "^5.1.6"
  },
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": ["<rootDir>/src/__tests__/setup.ts"],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{test,spec}.{js,jsx,ts,tsx}"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts",
      "!src/__tests__/**"
    ]
  }
}